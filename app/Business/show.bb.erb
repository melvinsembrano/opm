<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @business.name %></h1>
    <%= link_to "Edit", :action => :edit, :id => @business.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Name</td>
        <td class="itemValue"><%= @business.name%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Description</td>
        <td class="itemValue"><%= @business.description%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Lat</td>
        <td class="itemValue"><%= @business.lat%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Lng</td>
        <td class="itemValue"><%= @business.lng%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Url</td>
        <td class="itemValue"><%= @business.url%></td>
      </tr>
      
    </table>
  </div>

</div>
